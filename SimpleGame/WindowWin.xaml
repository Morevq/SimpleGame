<Window x:Class="SimpleGame.WindowWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleGame"
        mc:Ignorable="d"
        Title="WindowWin" Height="450" Width="800">
    <Grid>
        <Image Margin="104,10,104,44" Source="/youCool.jpg">
            <Image.OpacityMask>
                <ImageBrush ImageSource="/youCool.jpg"/>
            </Image.OpacityMask>
        </Image>
        <Button Content="Далее" Margin="642,250,23,136" Width="135"/>
        <Button x:Name="toMainWindow" Content="В главное меню" Margin="642,342,23,44" Width="135" Click="toMainWindow_Click"/>
        <Button Content="Назад" Margin="29,342,636,44" Width="135"/>
        <Button x:Name="Repeat" Content="Еще раз" Margin="29,250,636,136" Width="135" Click="Repeat_Click"/>

    </Grid>
</Window>
